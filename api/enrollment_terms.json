              "description": "ID",
              "type": "string",
              "format": null,
              "required": true
            },
            {
              "paramType": "query",
              "name": "workflow_state",
              "description": "no description",
              "type": "string",
              "format": null,
              "required": false,
              "tags": {
                "type": "array"
              }
            }
          ],
          "type": "array",
          "items": {
            "$ref": "EnrollmentTerm"
          }
