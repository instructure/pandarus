{
  "apiVersion": "1.0",
  "swaggerVersion": "1.2",
  "basePath": "http://canvas.instructure.com/api",
  "resourcePath": "/comm_messages",
  "produces": [
    "application/json"
  ],
  "apis": [
    {
      "path": "/v1/comm_messages",
      "description": "Retrieve messages sent to a user.",
      "operations": [
        {
          "method": "GET",
          "summary": "List of CommMessages for a user",
          "notes": "Retrieve messages sent to a user.",
          "nickname": "list_of_commmessages_for_user",
          "parameters": [
            {
              "paramType": "query",
              "name": "user_id",
              "description": "The user id for whom you want to retrieve CommMessages",
              "type": "string",
              "format": null,
              "required": true
            },
            {
              "paramType": "query",
              "name": "start_time",
              "description": "The beginning of the time range you want to retrieve message from.",
              "type": "datetime",
              "format": null,
              "required": false
            },
            {
              "paramType": "query",
              "name": "end_time",
              "description": "The end of the time range you want to retrieve messages for.",
              "type": "datetime",
              "format": null,
              "required": false
            }
          ],
          "type": "array",
          "items": {
            "$ref": "CommMessage"
          }
        }
      ]
    }
  ],
  "models": {
    "CommMessage": {
      "id": "CommMessage",
      "properties": {
        "id": {
          "type": "integer",
          "format": "int64"
        },
        "created_at": {
          "type": "string",
          "format": "date-time"
        },
        "sent_at": {
          "type": "string",
          "format": "date-time"
        },
        "workflow_state": {
          "type": "string"
        },
        "from": {
          "type": "string"
        },
        "to": {
          "type": "string"
        },
        "reply_to": {
          "type": "string"
        },
        "subject": {
          "type": "string"
        },
        "body": {
          "type": "string"
        },
        "html_body": {
          "type": "string"
        }
      }
    }
  }
}
